# MeetingBaas API Configuration
MEETINGBAAS_API_KEY=your_meetingbaas_api_key_here

# MeetingBaas Callback Configuration (for receiving bot.completed/bot.failed events)
# Set to your deployed URL + /api/webhooks/meetingbaas
MEETINGBAAS_CALLBACK_URL=https://your-domain.com/api/webhooks/meetingbaas
MEETINGBAAS_CALLBACK_SECRET=your_random_secret_here
# Claude API Configuration (for AI summarization and action item extraction)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google OAuth (for Calendar integration)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
